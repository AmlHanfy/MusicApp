<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.aml.musicapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".NowPlaying"
            android:label="@string/category_nowPlaying"/>
        <activity android:name=".MyPlayList"
            android:label="@string/category_myPlayList"/>
        <activity android:name=".Favourite"
            android:label="@string/category_favourite"/>
        <activity android:name=".Payment"
            android:label="@string/category_payment"></activity>
    </application>

</manifest>


